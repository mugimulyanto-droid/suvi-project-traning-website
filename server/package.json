{
  "name": "server",
  "version": "1.0.0",
  "description": "api for training website",
  "main": "index.ts",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "zidane abbas m",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.2",
    "prisma": "^6.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/typescript": "^0.4.29",
    "babel-jest": "^25.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "jest": "^25.0.0",
    "nodemon": "^3.1.10",
    "npm": "^11.6.0",
    "supertest": "^7.1.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
